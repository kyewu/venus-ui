{
  "name": "venus-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest --ui",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.10",
    "@unocss/reset": "66.1.0-beta.8",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.9.0",
    "pinia": "^3.0.1",
    "swiper": "^11.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.323",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@unocss/preset-icons": "66.1.0-beta.8",
    "@unocss/preset-wind4": "66.1.0-beta.8",
    "@unocss/transformer-directives": "66.1.0-beta.10",
    "@unocss/transformer-variant-group": "66.1.0-beta.11",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "markdown-it-prism": "^3.0.0",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "3.5.3",
    "prismjs": "^1.30.0",
    "sass-embedded": "^1.86.0",
    "start-server-and-test": "^2.0.10",
    "typescript": "~5.8.0",
    "unocss": "66.1.0-beta.8",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-markdown": "^28.3.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.1",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-mock-dev-server": "^1.8.7",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vue-layouts-next": "^0.0.11",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
